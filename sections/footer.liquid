{{ 'footer.css' | asset_url | stylesheet_tag }}

<div class="footer">
    <div class="page-width">
        <div class="container">
            <div class="footer__wrapper">
                <div class="footer__main-grid flex">
                    {%- for block in section.blocks -%}
                        {%- if block.type == "menu_list" -%}
                            <ul class="footer__menu-list footer__space-between">
                                {%- for link in linklists[block.settings.choose_menu].links -%}
                                    <li class="footer__menu-item">
                                        <a href="{{ link.url }}" class="footer__menu-link capitalize">{{ link.title }}</a>
                                    </li>
                                {%- endfor -%}
                            </ul>
                        {%- endif -%}
                    {%- endfor -%}
                    <div class="footer__content footer__space-between">
                        {%- if section.settings.title != blank -%}
                                <h3 class="footer__title">{{ section.settings.title }}</h3>
                        {%- endif -%}
                        <div class="footer__contact">
                            {%- if section.settings.text != blank -%}
                                <a href="{{ section.settings.url }}" class="footer__link">{{ section.settings.text }}</a>
                            {%- endif -%}
                            {%- if section.settings.contact != blank -%}
                                <span class="footer__link">{{ section.settings.contact }}</span>
                            {%- endif -%}
                        </div>
                    </div>
                </div>
                {%- if section.settings.title != blank and section.settings.text != blank and section.settings.contact -%}
